{
  "$schema": "http://json-schema.org/schema",
  "id": "Schema",
  "title": "Location page with SSR",
  "description": "Pre-generate pages describing locations",
  "properties": {
    "override": {
      "type": "boolean",
      "default": false,
      "description": "Confirm overriding existing pages Y/n",
      "x-prompt": "Are you sure you want to generate pages? (all other '/pages' will be overridden)"
    },
    "path": {
      "type": "string",
      "default": "pages",
      "description": "Path to generate pages",
      "x-prompt": "Where would you like your pages generated?"
    }
  },
  "required": [
    "override"
  ]
}
