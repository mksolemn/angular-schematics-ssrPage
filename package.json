{
  "name": "hello",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",

    "generate:hello:schematics": "schematics .:hello DemoSchematics --debug=false",
    "generate:hello:dts": "cd ./src/page && npx -p dtsgenerator dtsgen schema.json -o schema.d.ts",

    "generate:page:schematics": "schematics .:page PageDemoSchematics --debug=false",
    "generate:page:dts": "cd ./src/page && npx -p dtsgenerator dtsgen schema.json -o schema.d.ts",

    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^8.3.19",
    "@angular-devkit/schematics": "^8.3.19",
    "@angular/common": "^8.2.14",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.3.1",
    "typescript": "~3.5.3"
  }
}
